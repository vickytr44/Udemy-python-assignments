def gensquares(N):
    for items in range(N):
        yield items**2
for x in gensquares(10):
    print(x)

import random

def rand_num(low,high,n):
    for i in range(n):
        yield random.randint(low,high)
        
for num in rand_num(1,10,12):
    print(num)
    


s = 'hello'
my_iter= iter(s)
next(my_iter)


